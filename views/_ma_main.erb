<div class="row-fluid">
  <div class="span12 une_main"<%= tooltip("points = #{@partie.joueurs[0].a_pose_combien}") %>>
    <ul class="ma_main"><% @partie.joueurs[0].cartes.each do |carte| %>
      <li><%= img_carte(carte, @partie.carte_tiree) %></li><% end %>
    </ul>
  </div>
</div>

<div class="row-fluid">
  <div class="span12">
    <ul>
    <% @partie.coups.to_messages.reverse.take(12).each do |message| %>
      <li><%= message %></li>
    <% end %>
    </ul>
    <p><%= @partie.coups.reverse.to_s %></p>
  </div>
</div>
